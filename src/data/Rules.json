[
  {
    "id": "firstName",
    "message": "Surname can only contain alphabetic characters",
    "rule": "^[a-zA-Z]+(([a-zA-Z ])?[a-zA-Z]*)*$"
  },

  {
    "id": "lastName",
    "message": "Surname can only contain alphabetic characters",
    "rule": "^[a-zA-Z]+(([a-zA-Z ])?[a-zA-Z]*)*$"
  },

  {
    "id": "email",
    "message": "Invalid email address",
    "rule": "^(([^<>()\\[\\]\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$"
  },

  {
    "id": "phone",
    "message": "Phone number can only contain numbers, spaces and a '+' at the beginning",
    "rule": "^[+]*[\\s/0-9]*$"
  },

  {
    "id": "password",
    "message": "Password must be at least 8 characters long and container at least one special character",
    "rule": "^(?=.{8,})(?=.*[!\"#$%&'()*+,-./:;<=>?@[\\]^_`{|}~]).*$"
  }
]
